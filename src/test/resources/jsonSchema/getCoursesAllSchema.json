{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Courses All",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "^.*$"
      },
      "price": {
        "type": "integer",
        "minimum": 10000,
        "maximum": 1000000
      }
    },
    "additionalProperties": false,
    "required": [
      "name",
      "price"
    ]
  }
}